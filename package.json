{
  "name": "node-proxy-manager-server",
  "version": "0.0.1",
  "description": "The client dashboard for proxy manager.",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "jigsawye/node-proxy-manager-client",
  "author": {
    "name": "Jigsaw Ye",
