    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^2.1.5",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "dependencies": {
    "history": "^1.13.0",
    "isomorphic-fetch": "^2.2.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-modal": "^0.6.1",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.2",
    "redux-logger": "^2.0.3",
    "redux-promise": "^0.5.0",
    "redux-simple-router": "0.0.8",
    "redux-thunk": "^1.0.0"
